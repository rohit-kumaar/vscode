{
	"Reset": {
		"prefix": "reset",
		"body": [
			"html {",
			"  box-sizing: border-box;",
			"  font-size: 16px;",
			"}",
			"",
			"body {",
			"  color: hsl(0, 0%, 0%);",
			"  background: hsl(0, 0%, 100%);",
			"  font-family: \"Montserrat\", sans-serif;",
			"  font-weight: 400;",
			"}",
			"",
			"*,",
			"*::before,",
			"*::after {",
			"  padding: 0;",
			"  margin: 0;",
			"  box-sizing: inherit;",
			"}",
			"",
			"h1,",
			"h2,",
			"h3,",
			"h4,",
			"h5,",
			"h6,",
			"p,",
			"ol,",
			"ul,",
			"figure,",
			"blockquote {",
			"  padding: 0;",
			"  margin: 0;",
			"  font-weight: 400;",
			"}",
			"",
			"img,",
			"picture,",
			"svg {",
			"  max-width: 100%;",
			"  width: auto;",
			"  height: auto;",
			"}",
			""
		],
		"description": "Reset HTML Elements"
	},
	"Border red": {
		"prefix": "bdr",
		"body": [
			"border: 1px solid red;"
		],
		"description": "Border red"
	},
	"Border green": {
		"prefix": "bdg",
		"body": [
			"border: 1px solid green;"
		],
		"description": "Border green"
	},
	"Border yellow": {
		"prefix": "bdy",
		"body": [
			"border: 1px solid yellow;"
		],
		"description": "Border yellow"
	},
	"Root Element": {
		"prefix": "r",
		"body": [
			"rem($1)"
		],
		"description": "Root Element Snippets"
	},
	"Container": {
		"prefix": "container",
		"body": [
			".container {",
			"  width: min(100%, 1000px);",
			"  padding-inline: 1rem;",
			"  margin-inline: auto;",
			"}"
		],
		"description": "Set Layout Using Container"
	},
	"Margin top and bottom": {
		"prefix": "my",
		"body": [
			" margin-block: $1;"
		],
		"description": "Margin in y-axis"
	},
	"Margin left and right": {
		"prefix": "mx",
		"body": [
			" margin-inline: $1;"
		],
		"description": "Margin in x-axis"
	},
	"Padding top and bottom": {
		"prefix": "py",
		"body": [
			" padding-block: $1;"
		],
		"description": "Padding in y-axis"
	},
	"Padding left and right": {
		"prefix": "px",
		"body": [
			" padding-inline: $1;"
		],
		"description": "Padding in x-axis"
	},
	"comment": {
		"prefix": "comment",
		"body": [
			"/* ------------------- */",
			"/* Reset               */",
			"/* ------------------- */"
		],
		"description": "comment"
	},
	"sm": {
		"prefix": "sm",
		"body": [
			"@include media-breakpoint-up(sm) {",
			"$1",
			"}",
		],
		"description": "Breakpoint sm"

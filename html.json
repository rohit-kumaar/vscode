{
	"Ancher Tag": {
		"prefix": "atag",
		"body": [
			"<a href=\"javascript:void(0)\">$1</a>",
		],
		"description": "Ancher Tag"
	},
	"Image Tag": {
		"prefix": "imgtag",
		"body": [
			" <img loading=\"lazy\"  src=\"assets/images/$1\" alt=\"$2\" title=\"$3\" tabindex=\"-1\"/>"
		],
		"description": "Image Tag"
	},
	"hyper link": {
		"prefix": "href",
		"body": [
			"href=\"javascript:void(0)\""
		],
		"description": "hyper link"
	},
	"comment": {
		"prefix": "comment",
		"body": [
			"<!-- Start : $1 -->",
			"$2",
			"<!-- End   : $1 -->"
		],
		"description": "comment"
	},
	"link": {
		"prefix": "link",
		"body": [
			"[routerLink]=\"['/$1']\""
		],
		"description": ""
	},
	"Table Snippet": {
		"prefix": "table",
		"body": [
			"  <table style=\"\" class=\"\" align=\"center\" width=\"100%\" border=\"0\" cellpadding=\"0\" cellspacing=\"0\">",
			"    <thead>",
			"      <tr>",
			"        <th>",
			"",
			"        </th>",
			"      </tr>",
			"    </thead>",
			"    <tbody>",
			"      <tr>",
			"        <td>",
			"              $1",
			"        </td>",
			"      </tr>",
			"    </tbody>",
			"  </table>"
		],
		"description": "Table Snippet"
	},
	"Primary Button": {
		"prefix": "btn-primary",
		"body": [
			"<button class=\"button button--primary\">",
			"$1",
			"</button>"
		],
		"description": "Primary Button"
	},
	"Secondary Button": {
		"prefix": "btn-secondary",
		"body": [
			"<button class=\"button button--secondary\">",
			"$1",
			"</button>"
		],
		"description": "Secondary Button"
	},
	"Disable Button": {
		"prefix": "btn-disable",
		"body": [
			"<button class=\"button--disable\">",
			"$1",
			"</button>"
		],
		"description": "Disable Button"
	},
	"Transparent Button": {
		"prefix": "btn-transparent",
		"body": [
			"<button class=\"button--transparent\">",
			"$1",
			"</button>"
		],
		"description": "Transparent Button"
	}
}
